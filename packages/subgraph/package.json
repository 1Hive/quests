{
  "name": "@1hive/quests-subgraph",
  "license": "UNLICENSED",
  "version": "0.1.0",
  "scripts": {
    "prepare:rinkeby": "node ./sync-with-last-deploy.js && mustache config/rinkeby.json src/subgraph.template.yaml > subgraph.yaml",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy corantin/quests-subgraph --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "deploy-local": "graph deploy corantin/quests-subgraph --ipfs http://localhost:5001 --node http://127.0.0.1:8020"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.23.2",
    "@graphprotocol/graph-ts": "^0.23.1",
    "js-yaml": "^4.1.0",
    "mustache": "^4.2.0"
  }
}
